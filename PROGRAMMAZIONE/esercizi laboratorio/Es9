(define cifratura
  (lambda (key)     ;key: int [0-19]
       (let (    (alfabeto "ABCDEFGHILMNOPQRSTVX")
                 (minToMaiusc (-(char->integer #\a) (char->integer #\A) ))   ;numero da sottrarre per trasformare una lettera da min a maiusc
             )
            (succ
                 (lambda (string)
            )
       )
  )
)